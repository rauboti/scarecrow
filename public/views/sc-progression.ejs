<!DOCTYPE html>
<html>
  <head>
    <title><%=title%></title>
    <!-- Cleaner font -->
    <link href="https://fonts.googleapis.com/css?family=Rajdhani" rel="stylesheet">
    <!-- CSS source files -->
    <link rel="stylesheet" href="/css/main.css" />
    <link rel="stylesheet" href="/css/boti.css" />
    <link rel="stylesheet" href="/css/scarecrow.css" />
    <link rel="stylesheet" href="/css/responsive.css" />
    <nav>
      <menu>
        <a href="<%=scMenu['Home'].path%>"><div class="menu-button"><div class="menu-button-icon icon-home"></div></div></a>
        <% for (var i in scMenu) {
          if (scMenu[i].menu === 1) {
            if (i == activePage) {%>
            <a href="<%=scMenu[i].path%>"><div class="menu-button menu-button-selected"><div class="menu-button-text"><%=i%></div></div></a>
          <%} else {%>
            <a href="<%=scMenu[i].path%>"><div class="menu-button"><div class="menu-button-text"><%=i%></div></div></a>
        <%}}}%>
      </menu>
    </nav>
  </head>
  <body>
    <section id="pageContainer">
      <div class="container">
        <div class="container-frame">
          <div class="container-submenu">
            <% for (var i in progression) {%>
            <div id="btn-Progression-<%=i.replace(/[\' ]/g, '')%>" class="menu-button-small"><div class="menu-button-text"><%=i%></div></div>
            <%}%>
          </div>
          <div class="container-body">


              <% for (var i in progression) {
                for (var j in progression[i]) {%>
                  <div class="tblProgression boss-<%=i.replace(/[\' ]/g, '')%> hidden tr-highlight col-container">
                    <div class="col-40"><%=progression[i][j].boss%></div>
                    <div class="col-40"><%=progression[i][j].status%></div>
                  </div>
                  <%}
                } %>

          </div>
        </div>
      </div>
    </section>
  </body>
  <foot>
    <!-- JS source files -->
    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/scarecrow.js"></script>
    <script src="/js/mobile.js"></script>
    <script src="/js/main.js"></script>
  </foot>
</html>
