<!DOCTYPE html>
<html class="is-hidden">
  <head>
    <title><%=title%></title>
    <!-- Cleaner font -->
    <link href="https://fonts.googleapis.com/css?family=Rajdhani" rel="stylesheet">
    <!-- CSS source files -->
    <link rel="stylesheet" href="/css/main.css" />
    <link rel="stylesheet" href="/css/boti.css" />
    <link rel="stylesheet" href="/css/classes.css" />
    <link rel="stylesheet" href="/css/icons.css" />
    <link rel="stylesheet" href="/css/scarecrow.css" />
    <link rel="stylesheet" href="/css/responsive.css" />
    <nav>
      <menu>
        <a href="<%=scMenu['Home'].path%>"><div class="menu-button is-inline"><div class="menu-button-icon icon-logo"></div></div></a>
        <div class="show-mobile col-70 is-inline selected-page"><%=activePage%></div>
        <div class="menu-button is-inline show-mobile"><div class="menu-button-icon icon-menu"></div></div>
        <div class="menu-button-row show-desktop">
          <% for (var i in scMenu) {
            if (scMenu[i].menu === 1) {
              if (i == activePage) {%>
              <a href="<%=scMenu[i].path%>"><div class="menu-button button-selected-ghostly"><div class="menu-button-text text-selected"><%=i%></div></div></a>
            <%} else {%>
              <a href="<%=scMenu[i].path%>"><div class="menu-button"><div class="menu-button-text"><%=i%></div></div></a>
          <%}}}%>
        </div>
      </menu>
    </nav>
  </head>
  <body>
    <section>
      <div class="container-page align-center is-scrollable">
        <div class="container-big align-center">
          <% for (var i in users) {
            if (users[i].rank === 'Guild Master') {%>
              <div class="container-medium align-left is-framed border-rounded background-black">
                <div class="col-30 is-inline">Guild Master</div>
                <div class="col-30 is-inline"><%=users[i].user%></div>
                <div class="col-40 is-inline"></div>
              </div>
            <%}
          }%>
        </div>
        <div class="container-big align-center">
          <% for (var i in users) {
            if (users[i].rank === 'Officer') {%>
              <div class="container-medium align-left is-framed border-rounded background-black">
                <div class="col-30 is-inline">Officer</div>
                <div class="col-40 is-inline"><%=users[i].user%></div>
                <div class="col-30 is-inline"><%=users[i].role%></div>
              </div>
            <%}
          }%>
        </div>
      </div>
    </section>
  </body>
  <foot>
    <!-- JS source files -->
    <script src="/js/jquery-3.3.1.min.js"></script>
    <script src="/js/scarecrow.js"></script>
    <script src="/js/mobile.js"></script>
    <script src="/js/main.js"></script>
  </foot>
</html>
